#: models.py:37
msgid "Langtitude"
msgstr "Szerokość geograficzna"

#: models.py:38
msgid "URL of publication"
msgstr "Adres publikacji"

#: models.py:39
msgid "Public"
msgstr "Opublikowane"

#: models.py:40
msgid "Mark to publish on site"
msgstr "Zaznacz, aby opublikować na stronie"

